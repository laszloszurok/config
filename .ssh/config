AddKeysToAgent yes

Host github.com
    Hostname github.com
    User git
    IdentityFile ~/.ssh/id_ed25519

Host envs.net
    Hostname envs.net
    Port 2223
    User pulzar
    IdentityFile ~/.ssh/id_ed25519

Host rpi
    Hostname 192.168.0.5
    User pulzar
    LocalForward 9091 127.0.0.1:9091
    IdentityFile ~/.ssh/id_ed25519
